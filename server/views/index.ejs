                <li class="nav-item" ui-sref-active="active" ng-if="!appController.isAuthenticated()"><a href="" class="nav-link" ui-sref="login" close-navbar><i class="fa fa-lock fa-fw"></i>  Login</a></li>
                <li class="nav-item" ui-sref-active="active" ng-if="!appController.isAuthenticated()"><a href="" class="nav-link" ui-sref="signup" close-navbar><i class="fa fa-sign-in fa-fw"></i>  Sign Up</a></li>
                <li class="nav-item" ui-sref-active="active" ng-if="appController.isAuthenticated()"><a href="" class="nav-link" ui-sref="profile" close-navbar><i class="fa fa-user fa-fw"></i>  Profile</a></li>
                <li class="nav-item" ui-sref-active="active" ng-if="appController.isAuthenticated()"><a href="" class="nav-link" ui-sref="logout" close-navbar><i class="fa fa-sign-out fa-fw"></i>  Sign Out</a></li>
            </ul>
        </div>
    </nav>
</div>

<section id="page" ui-view autoscroll="true"></section>

<footer>
    <div class="container">
        <p class="lead">@Copyright 2015</p>
    </div>
</footer>

