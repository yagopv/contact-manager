<div class="container">
    <nav class="navbar navbar-light" role="navigation">
        <div class="row">
            <div class="col-xs-1 brand-toggler">
                <button class="navbar-toggler hidden-lg-up collapsed" type="button" data-toggle="collapse" data-target="#navbar-header">
                    &#9776;
                </button>
            </div>
            <div class="col-xs-11 text-xs-center brand-mobile">
                <a class="navbar-brand hidden-lg-up" href=""  ui-sref="home"><i class="fa fa-bullseye fa-fw"></i> Contact Manager</a>
            </div>
        </div>
        <div id="navbar-header" class="navbar-toggleable-md collapse">
            <a class="navbar-brand hidden-md-down" href=""  ui-sref="home" close-navbar><i class="fa fa-bullseye fa-fw"></i> Contact Manager</a>
            <ul class="nav navbar-nav">
                <li class="nav-item" ui-sref-active="active"><a href="" class="nav-link" ui-sref="home" close-navbar><i class="fa fa-home fa-fw"></i> Home</a></li>
                <li class="nav-item" ui-sref-active="active" ng-if="appController.isAuthenticated()"><a href="" class="nav-link" ui-sref="dashboard" close-navbar><i class="fa fa-dashboard fa-fw"></i>  Dashboard</a></li>
                <li class="nav-item" ui-sref-active="active" ng-if="appController.isAuthenticated()"><a href="" class="nav-link" ui-sref="newContact" close-navbar><i class="fa fa-user-plus fa-fw"></i>  New</a></li>
